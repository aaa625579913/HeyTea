.main-body {
    height: 70vh;
    width: 100%;
}

.cover-more {
    position: absolute;
    top: 30vh;
    height: 70vh;
    width: 100%;
    background: #ffffff;
    z-index: 10;
    animation-name: coverMore;
    animation-duration: 0.25s;
    animation-timing-function: linear;
    animation-iteration-count: 1;
    animation-fill-mode: forwards;
}

@keyframes coverMore {
    /** 第一种写法**/

    0% {
        opacity: 0.8;
    }

    50% {
        opacity: 0.9;
    }

    100% {
        opacity: 1.0;
    }
}

